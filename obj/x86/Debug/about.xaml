<Page
    x:Class="Lensinator.about"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Lensinator"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid Background="White">
        <Grid Height="29" Margin="0,-30,0,0" VerticalAlignment="Top" Background="#FF689F38"/>
        <Grid Background="#FF7CB342" Margin="0,-2,0,0" Height="66" VerticalAlignment="Top">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="271*"/>
                <ColumnDefinition Width="22*"/>
                <ColumnDefinition Width="108*"/>
            </Grid.ColumnDefinitions>
            <TextBlock x:Name="textBlock" Margin="12,12,52,14" TextWrapping="Wrap" Text="About" FontSize="33.333" FontFamily="DengXian" Grid.ColumnSpan="3"/>
            <TextBlock x:Name="textBlock_Copy" Margin="12,91,8,-106" TextWrapping="Wrap" Text="Thank You For Using Lensinator." FontSize="33.333" FontFamily="DengXian" Grid.ColumnSpan="3" Foreground="#FF7CB342"/>
            <TextBlock x:Name="textBlock_Copy2" Margin="12,206,8,-188" TextWrapping="Wrap" Text="Developed By :" FontSize="33.333" FontFamily="DengXian" Grid.ColumnSpan="3" Foreground="#FF7CB342"/>
            <TextBlock x:Name="textBlock_Copy3" Margin="12,261,8,-227" TextWrapping="Wrap" Text="Hrishi Patel" FontSize="30.667" FontFamily="DengXian" Grid.ColumnSpan="3" Foreground="#FF7CB342"/>
            <TextBlock x:Name="textBlock_Copy4" Margin="12,299,8,-310" TextWrapping="Wrap" Text="and inestimable support by Parth Patel." FontSize="24" FontFamily="DengXian" Grid.ColumnSpan="3" Foreground="#FF7CB342"/>
            <TextBlock x:Name="textBlock_Copy5" Margin="12,388,127,-354" TextWrapping="Wrap" Text="Version :" FontSize="30.667" FontFamily="DengXian" Foreground="#FF7CB342"/>
            <TextBlock x:Name="textBlock_Copy6" Margin="0,390,8,-356" TextWrapping="Wrap" Text="1.1.0.0" FontSize="30.667" FontFamily="DengXian" Foreground="#FF7CB342" Grid.ColumnSpan="3" HorizontalAlignment="Right" Width="244"/>
            <TextBlock x:Name="textBlock_Copy7" Margin="18.333,615,8,-571" TextWrapping="Wrap" Text="SerenApps" FontSize="16" FontFamily="DengXian" Foreground="#FF7CB342" Grid.Column="2"/>


        </Grid>

    </Grid>
</Page>

